{
  "name": "react-settings-pane",
  "version": "0.1.1",
  "description": "",
  "main": "lib/index",
  "files": [
    "*.md",
    "dist",
    "LICENSE",
    "src"
  ],
  "repository": "dstuecken/react-settings-pane",
  "authors": [
